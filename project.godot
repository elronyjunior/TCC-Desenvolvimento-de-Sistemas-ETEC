; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Rampage of the Realms"
run/main_scene="res://cenas/splash/splash_container.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
boot_splash/image="res://addons/awesome_splash/assets/None.png"
config/icon="res://icon/galinha.png"

[autoload]

StatusPlayer="*res://Player/status/status_player.gd"
Global="*res://scripts/Global.gd"
sp="*res://addons/awesome_splash/utils_splash.gd"
gd="*res://addons/godot-action/utils_gd_action.tscn"
ItensData="*res://data/itens/ItensData.gd"
Firebase="*res://addons/godot-firebase/firebase/firebase.tscn"
GlobalWorldEnvironment="*res://cenas/Config_Menu/world_environment.tscn"
MusicController="*res://Audios/scenes/music_controller.tscn"
LevelManager="*res://level_manager.gd"
TestLevel="*res://levels/scripts/test_level.gd"

[dialogic]

directories/dch_directory={
"character": "res://dialogos/character.dch",
"unit_test_character": "res://addons/dialogic-main/Tests/Resources/unit_test_character.dch"
}
directories/dtl_directory={
"inicio": "res://dialogos/inicio.dtl"
}
glossary/default_case_sensitive=true
layout/style_list=[]
layout/default_style="Default"
variables={
"Var": "nome"
}
extensions_folder="res://addons/dialogic_additions"
text/letter_speed=0.01
text/initial_text_reveal_skippable=true
text/text_reveal_skip_delay=0.1
text/advance_delay=0.1
text/autoadvance_per_character_delay=0.1
text/autoadvance_ignored_characters_enabled=true
animations/join_default_length=0.5
animations/join_default_wait=true
animations/leave_default_length=0.5
animations/leave_default_wait=true
animations/cross_fade_default_length=0.5
choices/autofocus_first=true
choices/delay=0.2
choices/reveal_delay=0
choices/reveal_by_input=false
save/autosave_delay=60.0
save/encryption_on_exports_only=true
text/autopauses={}
glossary/glossary_files=["res://dialogos/New_File.tres"]

[display]

window/size/mode=3

[editor_plugins]

enabled=PackedStringArray("res://addons/awesome_splash/plugin.cfg", "res://addons/godot-action/plugin.cfg", "res://addons/godot-firebase/plugin.cfg")

[file_customization]

folder_colors={
"res://Player/": "red",
"res://Tiny Swords (Update 010)/": "orange",
"res://bin/": "orange",
"res://cenas/": "pink",
"res://data/": "teal",
"res://projeteis/": "yellow",
"res://scripts/": "green",
"res://shader/": "green"
}

[importer_defaults]

animation_library={
"animation/fps": 60.0
}

